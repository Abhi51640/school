<table class="table table-striped">
  <thead>
   <tr>
     <th>Student Name</th>
     <th>Standard</th>
  
     <br />
    </tr>
  </thead>
  <tbody>
    <% @all_checkins.each do |checkin| %>
      <tr>
        <td><%= checkin.student.first_name %></td>
        <td><%= checkin.section_id %></td>
      </tr>
    <% end %>    
  </tbody>
</table>



<div class="container">
    <%#= turbo_stream_from "checkins" %>
    <div id="checkins">
    <%= render @all_checkins %>
  </div>
